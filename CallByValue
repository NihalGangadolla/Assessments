public class CallByValue {
	int data=1;  
	  
	 void change(int data){  
	 data=data+100;  
	 }  
	     
	 public static void main(String args[]){  
		 CallByValue  obj=new CallByValue();  
	  
	   System.out.println("before change "+obj.data);  
	   obj.change(500);  
	   System.out.println("after change "+obj.data);  
	  
	 }
}
