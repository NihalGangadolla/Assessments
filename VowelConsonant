public class VowelConsonant 
{
     public static void main(String[]args)
     {
    	 
    	 char ch = 'm';
    	 
    	 if(ch=='a' || ch=='e' || ch== 'i'|| ch =='o' || ch=='u')
    		 System.out.println("IT IS A VOWEL");
    	 
    	 else
    		 System.out.println("IT IS A CONSONANT");
    	 
    	 
    	 
    	 
     }
}
